Declare
    num1 number(2);
    num2 number(2);
    num3 number(2);
Begin 
    num1:=:num1;
    num2:=:num2;
    num3:=:num3;

    IF (num1>num2 and num1>num3) then
        dbms_output.put_line('Greater number is='||num1);
    elsif (num2 >num3 and num2>num1) then
        dbms_output.put_line('Greater number is='||num2);
    else 
      dbms_output.put_line('Greater number is='||num3);
 end if;
end;
